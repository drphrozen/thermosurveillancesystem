<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="1.2" 
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
    <managed-bean>
        <managed-bean-name>SessionBean1</managed-bean-name>
        <managed-bean-class>net2assignment1.SessionBean1</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>Login</managed-bean-name>
        <managed-bean-class>net2assignment1.Login</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ApplicationBean1</managed-bean-name>
        <managed-bean-class>net2assignment1.ApplicationBean1</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>RequestBean1</managed-bean-name>
        <managed-bean-class>net2assignment1.RequestBean1</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sap$SAPFrontpage</managed-bean-name>
        <managed-bean-class>net2assignment1.sap.SAPFrontpage</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/Login.jsp</from-view-id>
        <navigation-case>
            <from-outcome>caseFrontpage</from-outcome>
            <to-view-id>/sap/SAPFrontpage.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>NewJSFManagedBean</managed-bean-name>
        <managed-bean-class>net2assignment1.NewJSFManagedBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>LoginError</managed-bean-name>
        <managed-bean-class>net2assignment1.LoginError</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>sap$SAPReadingStation</managed-bean-name>
        <managed-bean-class>net2assignment1.sap.SAPReadingStation</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/sap/SAPFrontpage.jsp</from-view-id>
        <navigation-case>
            <from-outcome>caseEdit</from-outcome>
            <to-view-id>/sap/SAPReadingStation.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>caseLogout</from-outcome>
            <to-view-id>/Login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/sap/SAPReadingStation.jsp</from-view-id>
        <navigation-case>
            <from-outcome>caseFrontpage</from-outcome>
            <to-view-id>/sap/SAPFrontpage.jsp</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>caseLogout</from-outcome>
            <to-view-id>/Login.jsp</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>sap$Menu</managed-bean-name>
        <managed-bean-class>net2assignment1.sap.Menu</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
</faces-config>
